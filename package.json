      "cross-env": "^7.0.2",
      "gql2ts": "^1.10.1",
      "jest": "^26.1.0",
      "start-server-and-test": "^1.11.2",
      "ts-jest": "^26.1.3",
      "ts-node": "8.10.2",
      "ts-node-dev": "^1.0.0-pre.44",
      "tslint": "^6.1.2",
      "tslint-config-prettier": "^1.18.0",
      "typescript": "3.9.3"
   },
   "dependencies": {
      "@graphql-tools/graphql-file-loader": "^6.0.15",
      "@graphql-tools/load": "^6.0.15",
      "apollo-server": "^2.16.0",
      "bcryptjs": "^2.4.3",
      "graphql": "^15.3.0",
      "graphql-import": "^1.0.2",
